{
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
"routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": [
        "get",
        "post",
        "put",
        "patch",
        "options",
        "delete"
      ]
    }
  ]

    
    
}