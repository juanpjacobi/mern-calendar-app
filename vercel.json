{
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "src/main.ts",
          "methods": [
            "get",
            "post",
            "put",
            "patch",
            "options",
            "delete"
          ]
        }
      ]
    
    
    
}